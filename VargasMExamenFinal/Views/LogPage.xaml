<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VargasMExamenFinal.Views.LogPage"
             Title="Logs"
             BackgroundColor="{StaticResource PrimaryBackground}"
             Shell.BackgroundColor="{StaticResource PrimaryTitle}"
             Shell.ForegroundColor="White"
             Shell.TitleColor="White">
    <ScrollView>
        <Label x:Name="logLabel" Padding="10" TextColor="{StaticResource PrimaryText}"/>
    </ScrollView>
</ContentPage>